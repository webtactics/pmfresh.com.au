{% set postslist = collections['Leafy Salads'] | reverse  %} 
{% include "components/postslist-products-3-wide.njk" %}
</div>

{% elif recipespage %}
<div class="pad4rem"></div>
<div class="productcatagory content">
<h3>Recipes</h3>
{% set postslist = collections['Recipes']  %} 
{% include "components/postslist-recipes-3-wide.njk" %}
</div>



{% endif %}












	</main>
</div>



{% endblock %}




